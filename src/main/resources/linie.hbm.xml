<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pl.mzk.bielsko.Linie" table="linie">
        <meta attribute="class-description">
            This class contains bus lines
        </meta>
        <id name="id" type="int" column="id">
            <generator class="increment"/>
        </id>
        <property name="numer_linii" column="numer_linii" type="int"/>
        <property name="relacja" column="relacja" type="char"/>
        <property name="kierunek" column="kierunek" type="text"/>
    </class>
</hibernate-mapping>